  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for FirebaseChat
  pod 'Firebase' 
  pod 'Firebase/Auth' 
  pod 'Firebase/Database' 
  pod 'Firebase/Storage'
  pod 'Firebase/Firestore'
 
  pod 'IQKeyboardManagerSwift'
  pod 'SDWebImage'
end
